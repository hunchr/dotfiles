{
  "breadcrumbs.enabled": false,
  "chat.mcp.gallery.enabled": true,
  "debug.onTaskErrors": "debugAnyway",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  "editor.accessibilitySupport": "off",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.fontFamily": "MesloLGS NF, monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 14,
  "editor.fontWeight": 400,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": "off",
  "editor.rulers": [{ "column": 80 }, { "column": 100 }, { "column": 120 }],
  "editor.snippetSuggestions": "top",
  "editor.stickyScroll.enabled": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.tabSize": 2,
  "editor.unusualLineTerminators": "off",
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrap": "on",
  "emmet.showSuggestionsAsSnippets": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "onFocusChange",
  "files.eol": "\n",
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.enableSmartCommit": true,
  "git.ignoreRebaseWarning": true,
  "git.openRepositoryInParentFolders": "never",
  "git.path": "/opt/homebrew/bin/git",
  "git.replaceTagsWhenPull": true,
  "github.copilot.chat.copilotDebugCommand.enabled": false,
  "inlineChat.holdToSpeech": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "search.smartCase": true,
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.fontFamily": "MesloLGS NF, monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.shellIntegration.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.confirmSaveUntitledWorkspace": false,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editorAssociations": { "*.svg": "default" },
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "workbench.startupEditor": "none",

  // Copilot
  "github.copilot.chat.localeOverride": "en",
  "github.copilot.enable": { "*": false },

  // Formatters
  "[elixir]": { "editor.defaultFormatter": "JakeBecker.elixir-ls" },
  "[go]": { "editor.defaultFormatter": "golang.go" },
  "[ruby]": { "editor.defaultFormatter": "Shopify.ruby-lsp" },
  "[rust]": { "editor.defaultFormatter": "rust-lang.rust-analyzer" },
  "[toml]": { "editor.defaultFormatter": "tamasfe.even-better-toml" },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "evenBetterToml.formatter.columnWidth": 80,
  "prettier.enable": true,
  "prettier.requireConfig": true,
  "prettier.useEditorConfig": false,
  "rubyLsp.formatter": "rubocop_internal",

  // GitLens
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.plusFeatures.enabled": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.views.scm.grouped.views": {
    "branches": false,
    "commits": false,
    "contributors": false,
    "launchpad": false,
    "remotes": false,
    "repositories": false,
    "searchAndCompare": false,
    "stashes": false,
    "tags": false,
    "worktrees": false
  },
  "gitlens.views.searchAndCompare.avatars": false,
  "gitlens.views.searchAndCompare.files.layout": "tree",

  // Ruby
  "emmet.includeLanguages": { "erb": "html" },
  "files.associations": {
    "*.gemfile": "ruby",
    "Gemfile": "ruby"
  },
  "rubyLsp.featureFlags": { "all": true },
  "rubyLsp.rubyVersionManager": {
    "identifier": "mise",
    "miseExecutablePath": "/opt/homebrew/bin/mise"
  },

  // Rust
  "rust-analyzer.cargo.allTargets": false,
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.hideNamedConstructor": true,

